{
	"expo": {
		"name": "what-are-the-words",
		"slug": "what-are-the-words",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "what-are-the-words",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.zigliad.whatarethewords",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			},
			"usesAppleSignIn": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"expo-secure-store",
			"expo-apple-authentication",
			"react-native-google-mobile-ads"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "2ce79000-1d8e-455d-8a94-2e117f9036c2"
			}
		}
	},
	"react-native-google-mobile-ads": {
		"android_app_id": "ca-app-pub-4427652774441300~8812326958",
		"ios_app_id": "ca-app-pub-4427652774441300~3324196554"
	}
}
